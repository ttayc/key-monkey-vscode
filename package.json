{
  "name": "key-monkey",
  "displayName": "Key Monkey - Typing Test",
  "publisher": "ttayc",
  "description": "A typing test extension for VSCode",
  "icon": "icon.png",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ttayc/key-monkey-vscode"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "typing",
    "typing test",
    "ttayc",
    "speed",
    "practice"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "key-monkey.start",
        "title": "Key Monkey: Start"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run check-types && node esbuild.extension.js --production && node esbuild.webview.js --production",
    "compile": "npm run check-types && node esbuild.extension.js && node esbuild.webview.js",
    "compile:webview": "node esbuild.webview.js",
    "check-types": "tsc --noEmit",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/jquery": "^3.5.32",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "jquery": "^3.7.1"
  }
}
